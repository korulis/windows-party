<Window x:Class="WindowsParty.App.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:inf="clr-namespace:WindowsParty.Infrastructure;assembly=WindowsParty.Infrastructure"
        xmlns:navigation="clr-namespace:WindowsParty.Infrastructure.Navigation;assembly=WindowsParty.Infrastructure"
        mc:Ignorable="d"
        Title="{Binding CurrentTitle}"
        Width="800"
        Height="630"
        d:DataContext="{d:DesignInstance navigation:TitleResolver}">
    <Window.Resources>

    </Window.Resources>
    <Viewbox Stretch="Fill">

        <ContentControl prism:RegionManager.RegionName= "{x:Static inf:Regions.MainRegion}" />

    </Viewbox>
</Window>
